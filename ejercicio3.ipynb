    "def fibo(n):\n",
    "\n",
    "    if (n < 0):\n",
    "        return []\n",
    "    \n",
    "    resultado = [0, 1]\n",
    "    while (resultado[-2]+resultado[-1]) <= n:\n",
    "        resultado.append(resultado[-2]+resultado[-1])\n",
    "\n",
    "    return resultado\n",
    "        \n",
    "\n",
    "print(fibo(10))"
   ]
  }
